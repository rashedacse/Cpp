// Check prime number.

#include<iostream>
using namespace std;
int main()
{
    int n,i,isprime=1;
    cout<<"Enter num:\n";
    cin>>n;
    for(i=2;i<=n/2;i++)
    {

    if(n%i==0)
     {
        isprime=0;
        break;
     }
    }
    if(isprime==1)
     {
        cout<<n<<" is a prime number.\n";
     }
    else
     {
        cout<<n<<" is not a prime number.\n";
     }

}
