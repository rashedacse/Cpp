//Check Number wheather Posative, Negative or Zero.
#include <stdio.h>
#include<string.h>
#include <iostream>
#include<math.h>>
using namespace std;
int main()
{
    int n;
    cout<<"Enter any number:"; cin>>n;
    if(n>0) {cout<<"positive.\n";}
    else if(n<0){cout<<"negative.\n";}
    else {cout<<"zero.";}
}

