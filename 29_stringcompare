//string compare
#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    char a[100],b[100];
    cout<<"The 1st string:  ";
    cin>>a;
    cout<<"THe 2nd string:  ";
    cin>>b;
    if(strcmp(a,b)==0)
    {
        cout<<" The string r equal.";
    }
    else{
        cout<<"Not equal.";
    }
}
