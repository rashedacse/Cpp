//string compare
#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    char a[100],b[100];
    cout<<"The 1st string:  ";
    cin>>a;
    cout<<"THe 2nd string:  ";
    cin>>b;
    if(strcmp(a,b)==0)
    {
        cout<<" The string r equal.";
    }
    else{
        cout<<"Not equal.";
    }
}
//Program to Compare Two Strings Without using strcmp()
#include <stdio.h>
#include <stdlib.h>
int main()
{
    char a[100],b[100];
int i,temp = 1;
cout<<"Enter the string1 value:\n"; gets(a);
cout<<"\nEnter the String2 value:\n"; gets(b);
for(i=0; (a[i]!='\0')||(b[i]!='\0'); i++)
{
    if(a[i] != b[i])
    {   temp = 0;
        break;
    }
}
if(temp == 1)
cout<<"Both strings are same.";
else
cout<<"Both strings not same.";
}

