// Find perfect number.

#include<iostream>
using namespace std;
int main()
{ int n,i,min,max,sum;
  cout<<"Enter min number:";
  cin>>min;
  cout<<"Enter max number:";
  cin>>max;
  for(n=min;n<=max;n++)
  {  i=1,sum=0;
   while(i<n)
    {
      if(n%i==0)
      {
          sum=sum+i;
      }
      i++;
    }
  if(n==sum)
  {
      cout<<"\n"<<n<<"\n";
  }
 }

}
