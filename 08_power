#include<iostream>
#include<math.h>
using namespace std;
int main()
{

     int exponent;
     float base,result;
     cout<<"Enter base and exponent:";
     cin>>base>>exponent;
     //cout<<base<<"^"<<exponent<<"=";
    result=pow(base,exponent);
     cout<<result;
     return 0;
}
