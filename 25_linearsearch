//linear search.
#include<iostream>
using namespace std;
int main()
{
    int n,i,x,a[50];
    cout<<"Enter number of element:";
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"Enter element that u want to search:\n";
    cin>>x;
    for(i=0;i<n;i++)
    {
        if(a[i]==x)
            break;
    }
    if(i<n)
    {
        cout<<"Element found at position "<<i;
    }
    else{
        cout<<"Element not found. ";
    }
}
/*/Character linear search.
