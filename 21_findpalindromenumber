//check palindrome number.

#include<iostream>
using namespace std;
int main()
{ int n,i,rem,temp,sum=0;
  cout<<"Enter number:\n";
  cin>>n;
  temp=n;
  while(temp)
  {
      rem=temp%10;
      temp=temp/10;
      sum=sum*10+rem;
  }
  if(n==sum)
  {
      cout<<n<<" is a palindrome number.";
  }
  else
  {
      cout<<n<<" is not a palindrome number.";
  }
}
