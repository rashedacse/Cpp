//write a program in c to find the sum of series:1+(1+2)+(1+2+3)+..+(1+2+..+n)
#include<stdio.h>
#include<iostream>
using namespace std;
int main(){
 int n, sum=0,i,j;
 cout<<"Please enter an integer = "; cin>>n;
 for(i=1;i<=n;i++){

     for(j=1;j<=i;j++){
         sum = sum +j;
         //printf("\nsum11 = %d ===>%d, ", sum,j);
     }
 }
  cout<<"\nsum = "<< sum;
}
