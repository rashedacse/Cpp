// C++ Program - Add Two Numbers using Pointer */

#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int x,y,*p,*q,sum=0;
	cout<<"Enter two number: "; cin>>x>>y;
	p=&x;
	q=&y;
	sum=*p + *q;
	cout<<"The sum of two number: "<<sum;

}



