//Write a C program to enter length and breadth of a rectangle and find its perimeter.

#include<iostream>
using namespace std;
int main()
{

     int l,w,perimeter;
     cout<<"Enter length:";
     cin >>l;
     cout<<"Enter width:";
     cin >>w;
     perimeter=2*(l+w) ;
     cout<<"Area of rectangular:"<<perimeter<<endl;
     return 0;
}

