
//Write a C program to enter any number and calculate its square root.


#include<iostream>
#include<math.h>
using namespace std;
int main()
{

    int num;
    double ans;
    cout<<"Enter any Num: ";
    cin>>num;
   // ans=pow(num,0.5);
   ans=sqrt(num);
    cout<<"\n Squre of "<<num<<" is: "<<ans;

}

