//Finding GCD and LCM of Given Numbers using C
#include <iostream>
using namespace std;
int main()
{
    int l,s,rem=0,a,b;
    cout<<"Enter Two Non-Zero Integer Mumbers:"; cin>>l>>s;
    a=l; b=s;
    while(b!= 0)
    {   rem=a%b; a=b; b=rem;
    }
    cout<<"\n LCM of = "<<(l*s)/a;
}
