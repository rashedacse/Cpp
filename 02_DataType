//Write a C program to perform input/output of all basic data types.

#include<iostream>
using namespace std;
int main()
{

    int x;
    float a,b,y;
    char name[50];
    cout<<" plz enter ur name:";
    cin>>name;
    cout<<"Your name is"<<name<<endl;
    cout <<"enter 1st num:";
    cin >>a;
    cout<<"enter 2nd num:" ;
    cin >>b;
    x=a+b;
    y=a-b;
    cout<< "sumation of x="<<x<<endl;
    cout<< "division of y="<<y<<endl;
    return 0;



}

