// Fibonacii Seris
#include<iostream>
#include<iostream>
#include<stdio.h>
#include<string.h>
using namespace std;
int fibo(int );
int main()
{
  int i,n;
   cout<<"Take The Limit Of The Serise: \n ";   cin>>n;
    for(i=0;i<n;i++)
    {   cout<<fibo(i)<<" ";
    }
    return 0;
}
int fibo(int i)
{
    if(i==1 || i == 0)
    {
        return i;
    }
    else
        return (fibo(i-2)+fibo(i-1));
}

