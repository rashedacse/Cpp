//Fibbonecie number.
#include<iostream>
using namespace std;
int main()
{ int a=0,b=1,i,c,n;
  cout<<"Enter last digit: "; cin>>n;
  for(c=0;c<n;c++)
  {
      if(c<=1){ i=c; }
      else
      {
          i=a+b;
          a=b;
          b=i;
      }
       cout<<i<<" ";
   }
}
