
//Write a C program to enter Pricipal, Time, Rate and calculate Compound Interest.



#include<iostream>
#include<math.h>
using namespace std;

int main()
{

float p,r,t,ci;
cout<<"Enter value of p,t,r:" ;
cin>>p>>r>>t;
ci=p*pow((1+r/100),t)-p;
cout<<"Compound intrest (compounded anually) is :"<<ci;
return 0;
}
